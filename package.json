{
  "name": "nestjs-saas-starter",
  "version": "1.0.0",
  "description": "Production-ready NestJS starter for SaaS applications with authentication, RBAC, and CRUD base classes",
  "author": "",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "typescript",
    "saas",
    "starter",
    "template",
    "typeorm",
    "authentication",
    "authorization",
    "rbac",
    "casl",
    "jwt",
    "oauth"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nestjs-saas-starter"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "start:prod": "nest start",
    "start:dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/data-source.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/data-source.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/data-source.ts",
    "lint": "pnpm -r --stream exec eslint . --fix",
    "typecheck": "pnpm -r --stream exec tsc --noEmit",
    "clean": "rimraf --glob '**/dist' '**/node_modules' 'pnpm-lock.yaml'",
    "setup": "bash scripts/setup.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.665.0",
    "@casl/ability": "^6.7.2",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/typeorm": "^11.0.0",
    "@vimeo/vimeo": "^3.0.3",
    "axios": "^1.8.3",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "express": "^4.21.1",
    "glob": "^10.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "js-yaml": "^4.1.0",
    "ms": "^2.1.3",
    "multer": "1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.16.1",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "simple-git": "^3.27.0",
    "stripe": "^18.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.28",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@types/express": "^4.17.13",
    "@types/glob": "^9.0.0",
    "@types/jest": "29.5.1",
    "@types/js-yaml": "^4.0.9",
    "@types/ms": "^2.1.0",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "18.16.12",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-facebook": "^3.0.4",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.11",
    "@types/vimeo": "^2.1.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.5.0",
    "node-loader": "^2.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.1.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.5.4",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.3.3",
    "webpack": "^5.105.0"
  }
}
